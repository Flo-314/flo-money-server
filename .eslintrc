{
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "plugins": ["promise", "prettier"],
  "env": {
    "node": true,
    "jest": true
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "printWidth": 120,
        "singleQuote": true,
        "trailingComma": "es5"
      }
    ],
    "func-names":"off"
  }
}
